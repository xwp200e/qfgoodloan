<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.dao.LoanedStatuDao">
    <select id="getAllLoanedByCidTrue" resultType="loaned">
        select * from loaned where cid = #{cid} and lstatus = true
    </select>

    <select id="getAllLoanedByCidNotTrue" resultType="loaned">
        select * from loaned where cid = #{cid} and lstatus = false
    </select>
</mapper>